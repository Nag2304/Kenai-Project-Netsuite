<ShippingLabel>
    <Header>
        <LabelHeader>
            <RequestId>{{RequestId}}</RequestId>
            <LabelId>{{LabelId}}</LabelId>
            <ShipmentIdentification>{{ShipmentIdentification}}</ShipmentIdentification>
            <ShipmentDate>{{ShipmentDate}}</ShipmentDate>
            <Vendor>{{Vendor}}</Vendor>
			<ShipmentLadingQuantity>{{ShipmentLadingQuantity}}</ShipmentLadingQuantity>
            <CarrierAlphaCode>{{CarrierAlphaCode}}</CarrierAlphaCode>
            <CarrierRouting>{{CarrierRouting}}</CarrierRouting>
            <BillOfLadingNumber>{{BillOfLadingNumber}}</BillOfLadingNumber>
            <CarrierProNumber>{{CarrierProNumber}}</CarrierProNumber>
            <AppointmentNumber>{{AppointmentNumber}}</AppointmentNumber>
            <CurrentScheduledDeliveryDate>{{CurrentScheduledDeliveryDate}}</CurrentScheduledDeliveryDate>
            <CartonCount>{{CartonCount}}</CartonCount>
            <CartonTotal>{{CartonTotal}}</CartonTotal>
			<Comment1>{{Comment1}}</Comment1>
			<Comment2>{{Comment2}}</Comment2>
			<Comment3>{{Comment3}}</Comment3>
			<Comment4>{{Comment4}}</Comment4>
			<Comment5>{{Comment5}}</Comment5>
            <FlexField1>{{FlexField1}}</FlexField1>
            <FlexField2>{{FlexField2}}</FlexField2>
			<FlexField3>{{FlexField3}}</FlexField3>
        </LabelHeader>
        {{#ShipFrom}}
        <ShipFromAddress>
            <AddressLocationNumber>{{ShipFromAddressLocationNumber}}</AddressLocationNumber>
            <AddressName>{{ShipFromAddressName}}</AddressName>
			<AddressAlternateName>{{ShipFromAddressAlternateName}}</AddressAlternateName>
            <Address1>{{ShipFromAddress1}}</Address1>
            <Address2>{{ShipFromAddress2}}</Address2>
			<Address3>{{ShipFromAddress3}}</Address3>
            <City>{{ShipFromCity}}</City>
            <State>{{ShipFromState}}</State>
            <PostalCode>{{ShipFromPostalCode}}</PostalCode>
            <Country>{{ShipFromCountry}}</Country>
			<ContactName>{{ShipFromContactName}}</ContactName>
            <AddressAlternateName2>{{ShipFromAddressAlternateName2}}</AddressAlternateName2>
        </ShipFromAddress>
        {{/ShipFrom}}
        {{#ShipTo}}
        <ShipToAddress>
            <AddressLocationNumber>{{ShipToAddressLocationNumber}}</AddressLocationNumber>
            <AddressName>{{ShipToAddressName}}</AddressName>
            <AddressAlternateName>{{ShipToAddressAlternateName}}</AddressAlternateName>
            <Address1>{{ShipToAddress1}}</Address1>
            <Address2>{{ShipToAddress2}}</Address2>
			<Address3>{{ShipToAddress3}}</Address3>
            <City>{{ShipToCity}}</City>
            <State>{{ShipToState}}</State>
            <PostalCode>{{ShipToPostalCode}}</PostalCode>
            <Country>{{ShipToCountry}}</Country>
            <ContactName>{{ShipToContactName}}</ContactName>
			<AddressAlternateName2>{{ShipToAddressAlternateName2}}</AddressAlternateName2>
        </ShipToAddress>
        {{/ShipTo}}
		{{#Additional}}
        <AdditionalAddress>
            <AddressLocationNumber>{{AdditionalAddressLocationNumber}}</AddressLocationNumber>
            <AddressName>{{AdditionalAddressName}}</AddressName>
            <AddressAlternateName>{{AdditionalAddressAlternateName}}</AddressAlternateName>
            <Address1>{{AdditionalAddress1}}</Address1>
            <Address2>{{AdditionalAddress2}}</Address2>
			<Address3>{{AdditionalAddress3}}</Address3>
            <City>{{AdditionalCity}}</City>
            <State>{{AdditionalState}}</State>
            <PostalCode>{{AdditionalPostalCode}}</PostalCode>
            <Country>{{AdditionalCountry}}</Country>
            <ContactName>{{AdditionalContactName}}</ContactName>
			<AddressAlternateName2>{{AdditionalAddressAlternateName2}}</AddressAlternateName2>
        </AdditionalAddress>
        {{/Additional}}
    </Header>
    <Order>
        <OrderHeader>
            <InvoiceNumber>{{InvoiceNumber}}</InvoiceNumber>
            <PurchaseOrderNumber>{{PurchaseOrderNumber}}</PurchaseOrderNumber>
            <ReleaseNumber>{{ReleaseNumber}}</ReleaseNumber>
            <PurchaseOrderDate>{{PurchaseOrderDate}}</PurchaseOrderDate>
            <Department>{{Department}}</Department>
			<Vendor>{{Vendor}}</Vendor>
            <Division>{{Division}}</Division>
            <BusinessFamily>{{BusinessFamily}}</BusinessFamily>
            <CustomerAccountNumber>{{CustomerAccountNumber}}</CustomerAccountNumber>
            <CustomerOrderNumber>{{CustomerOrderNumber}}</CustomerOrderNumber>
            <PromotionDealNumber>{{PromotionDealNumber}}</PromotionDealNumber>
			<OrderStatusCode>{{OrderStatusCode}}</OrderStatusCode>
			<NumberOfUnitsShipped>{{NumberOfUnitsShipped}}</NumberOfUnitsShipped>
            <ShipToName>{{ShipToName}}</ShipToName>
			<ShipToAddressLocationNumber>{{ShipToAddressLocationNumber}}</ShipToAddressLocationNumber>
            <StoreName>{{Store.StoreName}}</StoreName>
            <StoreNumber>{{Store.StoreNumber}}</StoreNumber>
            <StoreAddress1>{{Store.StoreAddress1}}</StoreAddress1>
            <StoreAddress2>{{Store.StoreAddress2}}</StoreAddress2>
            <StoreCity>{{Store.StoreCity}}</StoreCity>
            <StoreState>{{Store.StoreState}}</StoreState>
            <StorePostalCode>{{Store.StorePostalCode}}</StorePostalCode>
            <StoreCountry>{{Store.StoreCountry}}</StoreCountry>
            <EventCode>{{EventCode}}</EventCode>
        </OrderHeader>
        {{#Packs}}
        <Pack>
			<OuterPack>{{OuterPack}}</OuterPack>
			<PackSize>{{PackSize}}</PackSize>
            <PackWeight>{{PackWeight}}</PackWeight>
            <InnerPack>{{InnerPack}}</InnerPack>
			<PackagingCharacteristicCode>{{PackagingCharacteristicCode}}</PackagingCharacteristicCode>
            <Comment1>{{Comment1}}</Comment1>
            <Comment2>{{Comment2}}</Comment2>
            <Comment3>{{Comment3}}</Comment3>
            <Comment4>{{Comment4}}</Comment4>
            <Comment5>{{Comment5}}</Comment5>
            <FlexField1>{{PackFlexField1}}</FlexField1>
            <FlexField2>{{PackFlexField2}}</FlexField2>
			<FlexField3>{{PackFlexField3}}</FlexField3>
        </Pack>
        {{/Packs}}
        {{#Packs}}
        <UCCAndOrEANBarcodeItem>
            <SerialShippingContainerCode>{{SerialShippingContainerCode}}</SerialShippingContainerCode>
			<ShippingContainerCode>{{ShippingContainerCode}}</ShippingContainerCode>
            <ExpirationDate>{{ExpirationDate}}</ExpirationDate>
			<LotNumber>{{LotNumber}}</LotNumber>
        </UCCAndOrEANBarcodeItem>
        {{/Packs}}
        <UPC-ACaseCode>{{UPC-ACaseCode}}</UPC-ACaseCode>
		<UPC-ACaseCodeText>{{UPC-ACaseCodeText}}</UPC-ACaseCodeText>
        {{#Items}}
        <Item>
            <BuyerPartNumber>{{BuyerPartNumber}}</BuyerPartNumber>
            <VendorPartNumber>{{VendorPartNumber}}</VendorPartNumber>
            <ConsumerPackageCode>{{ConsumerPackageCode}}</ConsumerPackageCode>
            <GTIN>{{GTIN}}</GTIN>
			<UPCCaseCode>{{UPCCaseCode}}</UPCCaseCode>
            <OuterPack>{{OuterPack}}</OuterPack>
            <InnerPack>{{InnerPack}}</InnerPack>
            <ShipQty>{{ShipQty}}</ShipQty>
            <ShipQtyUOM>{{ShipQtyUOM}}</ShipQtyUOM>
            <ProductSizeDescription>{{ProductSizeDescription}}</ProductSizeDescription>
            <ProductColorDescription>{{ProductColorDescription}}</ProductColorDescription>
            <ProductWidthDescription>{{ProductWidthDescription}}</ProductWidthDescription>
            <ProductStyleDescription>{{ProductStyleDescription}}</ProductStyleDescription>
            <ProductDescription>{{ProductDescription}}</ProductDescription>
			<Department>{{Department}}</Department>
            <Class>{{Class}}</Class>
            <FlexField1>{{ItemFlexField1}}</FlexField1>
            <FlexField2>{{ItemFlexField2}}</FlexField2>
			<FlexField3>{{ItemFlexField3}}</FlexField3>
        </Item>
        {{/Items}}
    </Order>
</ShippingLabel>